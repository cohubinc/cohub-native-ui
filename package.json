{
  "name": "@cohubinc/cohub-native-ui",
  "version": "1.13.0",
  "description": "Sharable native UI components",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "author": "Cohub",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=development rollup -cw",
    "build": "NODE_ENV=production rollup -c",
    "build:stories": "NODE_ENV=storybook rollup -c",
    "watch:stories": "NODE_ENV=storybook rollup -cw",
    "fresh": "rm -rf node_modules && yarn cache clean && yarn",
    "storybook": "start-storybook -p 6007",
    "storybook:build": "yarn build:stories && build-storybook -c .storybook -o .storybook/stories-dist",
    "pre-push-msg": "echo 'Pre-push checks...' && exit 0",
    "pre-push-test": "yarn test --no-watch",
    "lint": "tslint --project tsconfig.build.json",
    "lint:fix": "yarn lint --fix",
    "test:coverage": "yarn test --coverage",
    "checkTypes": "yarn tsc --noEmit",
    "release": "yarn && yarn build && yarn publish",
    "test": "NODE_ENV=test jest --config jest.json --watch"
  },
  "lint-staged": {
    "./src/*.{ts,tsx}": [
      "yarn lint:fix",
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push-msg": "echo 'Building dist. You will likely need to amend your previous commit and force push the changes to dist.' && exit 0",
      "pre-push": "yarn checkTypes && yarn test --no-watch && yarn build"
    }
  },
  "peerDependencies": {
