{
  "presets": ["@babel/preset-react", "@babel/preset-typescript"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src/", "./dist/"],
        "extensions": [".ts", ".tsx"],
        "alias": {
          "src": "./src",
          "dist": "./dist"
        }
      }
    ],
    "@babel/plugin-transform-runtime"
  ]
}
